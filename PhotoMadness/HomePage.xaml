<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PhotoMadness.HomePage"
             NavigationPage.HasBackButton="False"
             Title="Tasks"
             xmlns:local="clr-namespace:PhotoMadness"
            BackgroundColor="#F0A64D">

    <NavigationPage Title="Shop">
        <x:Arguments>
            <local:ShopPage />
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="Camera">
        <x:Arguments>
            <local:CamPage />
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="Tasks">
        <x:Arguments>
            <local:TaskPage />
        </x:Arguments>
    </NavigationPage>

</TabbedPage>